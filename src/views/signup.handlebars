<div id="container" class="container">
    <h1>Sign Up</h1>
    <form id="sigUpForm" action="" method="post">
        <div class="control-input">
            <input id="input-name" class="input" name="firstName"  type="text" placeholder="Name..."/>
            <p class="error-message invisible"></p>
        </div>
        <div class="control-input">
            <input id="input-lastname" class="input" name="lastName"  type="text" placeholder="Last Name..."/>
            <p class="error-message invisible"></p>
        </div>
        <div class="control-input">
            <input id="input-email" class="input" name="email"  type="email" placeholder="Email..."/>
            <p class="error-message invisible"></p>
        </div>
        <div class="control-input">
            <input id="input-birthdate" class="input" name="birthdate"  type="date" placeholder="Birthdate..."/>
            <p class="error-message invisible"></p>
        </div>
        <div class="control-input">
            <input id="input-password" class="input" name="password"  type="password" placeholder="Password..."/>
            <p class="error-message invisible"></p>
        </div>
        <div class="control-input">
            <input id="input-confirmPassword" class="input" name="confirmPassword"  type="password" placeholder="Confirm password..."/>
            <p class="error-message invisible"></p>
        </div>
        <button id="btn-signup" class="btn btn-primary" type="submit">Sign Up</button>
    </form>
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script type="module" src="/js/signup/index.js"></script>
